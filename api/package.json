  "bugs": {
    "url": "https://github.com/mi-mazouz/Matcha/issues"
  },
  "homepage": "https://github.com/mi-mazouz/Matcha#readme",
  "dependencies": {
    "bcrypt": "3.0.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "express-jwt": "^5.3.1",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "pg": "^7.4.3",
    "sequelize": "^4.38.0",
    "winston": "3.0.0"
